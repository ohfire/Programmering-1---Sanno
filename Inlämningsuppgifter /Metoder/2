using System;

class Program
{
    static void Main(string[] args)
    {
        int tal1 = 5;
        int tal2 = 10;
        Console.WriteLine($"Det största talet är: {Störst(tal1, tal2)}");
    }

    static int Störst(int tal1, int tal2)
    {
        if (tal1 > tal2)
        {
            return tal1;
        }
        else if (tal2 > tal1)
        {
            return tal2;
        }
        else
        {
            return tal1; // Eller tal2 eftersom de är lika
        }
    }
}
